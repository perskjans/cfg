

super + d
    dmenu_run -c -l 30

super + alt + m
    p-dmenu-unicode

super + alt + t
    p-toggletray

super + shift + q
    p-dmenu-exit

XF86AudioLowerVolume
    p-audioctl down

XF86AudioRaiseVolume
    p-audioctl up

XF86AudioMute
    p-audioctl toggle

XF86MonBrightnessDown
    xbacklight -dec 5

XF86MonBrightnessUp
    xbacklight -inc 5
